{
  "name": "@ai-pipestream/protobuf-forms",
  "version": "1.0.0",
  "description": "Type-safe form generation from Protocol Buffer messages using JSONForms",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "import": "./src/vue.ts",
      "types": "./src/vue.ts"
    },
    "./generated": {
      "import": "./src/generated/index.ts",
      "types": "./src/generated/index.ts"
    },
    "./generated/*": {
      "import": "./src/generated/*.ts",
      "types": "./src/generated/*.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "proto:sync": "bash scripts/sync-protos.sh",
    "proto:deps": "buf dep update",
    "proto:generate": "buf generate && bash scripts/create-index.sh",
    "proto:build": "pnpm proto:sync && pnpm proto:deps && pnpm proto:generate",
    "build": "pnpm proto:build && tsup",
    "clean": "rm -rf dist src/generated proto buf.lock .proto-temp",
    "dev": "tsup --watch",
    "test": "echo 'No tests yet'",
    "typecheck": "echo 'Type checking skipped - Vue components not included in build'",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-pipestream/platform-frontend.git",
    "directory": "packages/protobuf-forms"
  },
  "bugs": {
    "url": "https://github.com/ai-pipestream/platform-frontend/issues"
  },
  "homepage": "https://github.com/ai-pipestream/platform-frontend/tree/main/packages/protobuf-forms#readme",
  "license": "Apache-2.0",
  "author": "Pipeline Team",
  "keywords": [
    "protobuf",
    "forms",
    "jsonforms",
    "vue",
    "type-safe",
    "pipeline"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "@bufbuild/protobuf": "catalog:"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.61.0",
    "@bufbuild/protoc-gen-es": "^2.10.2",
    "@types/node": "^22.10.2",
    "degit": "^2.8.4",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@jsonforms/core": "catalog:",
    "@jsonforms/vue": "catalog:",
    "@jsonforms/vue-vuetify": "catalog:",
    "vue": "catalog:"
  },
  "engines": {
    "node": ">=22.0.0 <23",
    "pnpm": ">=10 <11"
  }
}
