{
  "name": "@ai-pipestream/connector-shared",
  "version": "1.0.0",
  "description": "Backend utilities for connector functionality including streaming protocols, upload clients, and document processing",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "echo 'No tests yet'",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-pipestream/platform-frontend.git",
    "directory": "packages/connector-shared"
  },
  "bugs": {
    "url": "https://github.com/ai-pipestream/platform-frontend/issues"
  },
  "homepage": "https://github.com/ai-pipestream/platform-frontend/tree/main/packages/connector-shared#readme",
  "license": "Apache-2.0",
  "author": "Pipeline Team",
  "keywords": [
    "pipeline",
    "connector",
    "streaming",
    "upload",
    "document-processing"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "@ai-pipestream/protobuf-forms": "workspace:*",
    "@bufbuild/protobuf": "catalog:",
    "@connectrpc/connect": "catalog:",
    "@connectrpc/connect-node": "catalog:",
    "@connectrpc/connect-web": "catalog:",
    "chalk": "^5.6.2",
    "mime-types": "^3.0.2"
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0 <23",
    "pnpm": ">=10 <11"
  }
}