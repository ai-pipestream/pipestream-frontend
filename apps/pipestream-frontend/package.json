{
  "name": "@ai-pipestream/pipestream-frontend",
  "version": "1.0.0",
  "description": "Platform Shell - Unified web frontend and Connect-ES proxy for Pipeline Engine services",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc --skipLibCheck --noEmitOnError false",
    "build:ui": "cd ui && pnpm run build",
    "build:all": "pnpm run build && pnpm run build:ui",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test:upload:dev": "tsx scripts/test-upload-chunks.ts",
    "start:dev": "tsx src/index.ts",
    "docker:build": "docker build -t ai-pipestream/pipestream-frontend .",
    "docker:run": "docker run -p 38106:38106 ai-pipestream/pipestream-frontend",
    "docker:compose": "docker-compose up -d",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run dev:ui\"",
    "dev:backend": "tsx watch src/index.ts",
    "dev:ui": "cd ui && pnpm run dev"
  },
  "keywords": [
    "pipeline",
    "platform",
    "frontend",
    "connect-es",
    "vue"
  ],
  "author": "Pipeline Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-pipestream/platform-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/ai-pipestream/platform-frontend/issues"
  },
  "homepage": "https://github.com/ai-pipestream/platform-frontend#readme",
  "dependencies": {
    "@ai-pipestream/connector-shared": "workspace:*",
    "@ai-pipestream/protobuf-forms": "workspace:*",
    "@bufbuild/protobuf": "catalog:",
    "@connectrpc/connect": "catalog:",
    "@connectrpc/connect-express": "catalog:",
    "@connectrpc/connect-node": "catalog:",
    "@types/multer": "^2.0.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "multer": "^2.0.2",
    "tsx": "^4.21.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0 <23",
    "pnpm": ">=10 <11"
  }
}